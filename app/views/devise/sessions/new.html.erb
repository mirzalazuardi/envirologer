<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title">Login</h3>
  </div>
  <!-- /.box-header -->
  <!-- form start -->
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="box-body">
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <%= f.email_field :email, autofocus: true,class: "form-control", id: "exampleInputEmail1", placeholder: "Enter email" %>
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Password</label>
      <%= f.password_field :password, autocomplete: "off", class: "form-control" ,id: "exampleInputPassword1" ,placeholder: "Password" %>
    </div>
    <!-- /.box-body -->

    <div class="box-footer">
      <div class="actions">
        <%= f.submit "Log in", class: "btn btn-primary" %>
      </div>
      <br>
      <%= render "devise/shared/links" %>
    </div>
  <% end %>
  </div>
